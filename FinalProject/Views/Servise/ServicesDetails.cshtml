@model ServicesDetailsViewModel

@{
    Layout = "_AdminLayout";

}
<div class="card border-primary mb-3" style="width:100%; max-height: 20rem; border:none">


    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/Dashboard/AdminIndex">Dashboard</a></li>
        <li class="breadcrumb-item"><a href="/Drug/AllDrug">Services List</a></li>
        <li class="breadcrumb-item"><a href="#">Services Info</a></li>

    </ol>
</div>

<div class="card text-center mx-auto" style="max-width: 600px;">
    <div class="card-body">
        <h3 class="card-title mb-4  text-primary"> Services Info</h3>
        <hr />
        <div class="doc-info-body">
            <div class="doc-info-item">
                <ul class="list-unstyled d-grid gap-10">
                    <li class="d-sm-flex">
                        <i class="fas fa-user me-2 text-primary"></i>
                        <span class="fw-bold doc-info-title">Name</span>
                        <span class="fw-bold d-inline-block me-10"> : </span>
                        <span class="fs-base">@Model?.Name</span>
                    </li>
                    <li class="d-sm-flex">
                        <i class="fas fa-tag me-2 text-primary"></i>
                        <span class="fw-bold doc-info-title">Price</span>
                        <span class="fw-bold d-inline-block me-10"> : </span>
                        <span class="fs-base">@Model?.Price</span>
                    </li>




                </ul>
            </div>
        </div>
    </div>
</div>

